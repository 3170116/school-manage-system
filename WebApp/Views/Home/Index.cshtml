@using WebApp.ViewModels
@model IndexViewModel

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js"></script>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.19.1/jquery.validate.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validation-unobtrusive/3.2.11/jquery.validate.unobtrusive.min.js"></script>

    <title>Είσοδος</title>
    <link rel="icon" href="https://i.pinimg.com/originals/37/86/00/3786001447986993c2cd5bbfe77c9645.png" />

    <link rel="stylesheet" href="~/css/site.css" />
    <script type="text/javascript" src="~/js/Views/Home/index.js"></script>
</head>

<body>

    <div class="row justify-content-center">
        <div class="col-md-2 col-sm-8 justify-content-center">
            <img src="data:image/jpeg;base64," alt="Logo" class="img-fluid" id="index-logo">
        </div>
    </div>

    <div class="row justify-content-center">
        <div class="col-md-6 col-sm-12 border">

            <ul class="nav nav-tabs justify-content-center" id="login-tabs">

                <li class="nav-item">
                    <a class="nav-link active" id="logInTab">Σύνδεση</a>
                </li>

                <li class="nav-item">
                    <a class="nav-link" id="signUpTab">Εγγραφή</a>
                </li>

            </ul>

            <div class="tab-content" id="index-form">

                <div class="tab-pane fade show active" id="logIn-content">

                    <form method="post" action="@Url.Action("LogIn","Home")">

                        <div asp-validation-summary="ModelOnly" class="text-danger"></div>

                        <div class="row justify-content-center">
                            <div class="col-12">
                                <input type="email" class="form-control" placeholder="e-mail" name="Teacher.Email" data-val="true" data-val-required="This field is required *" id="Teacher_Email">
                                <label data-valmsg-for="Teacher.Email" data-valmsg-replace="true" class="text-danger"></label>
                            </div>
                        </div>

                        <div class="row justify-content-center">
                            <div class="col-12">
                                <input type="password" class="form-control" placeholder="password" name="Teacher.Password" data-val="true" data-val-required="This field is required *" id="Teacher_Password">
                                <label data-valmsg-for="Teacher.Password" data-valmsg-replace="true" class="text-danger"></label>
                            </div>
                        </div>

                        <div class="row justify-content-center">
                            <div class="col-12">
                                <input type="submit" class="btn btn-success" value="Σύνδεση" />
                                @if (Model.AccountExists != null && Model.AccountExists.Equals("False"))
                                {
                                    <label class="text-danger text-center">This account does not exist!</label>
                                }
                            </div>
                        </div>

                    </form>

                </div>

                <div class="tab-pane fade" id="signUp-content">

                    <form method="post" action="@Url.Action("Register","Home")">

                        <div asp-validation-summary="ModelOnly" class="text-danger"></div>

                        <div class="row justify-content-center">

                            <div class="col-12">
                                <label class="label-info">Πληροφορίες για το σχολείο:</label>
                            </div>

                            <div class="col-12">
                                <input type="text" class="form-control" placeholder="Όνομα (πχ 15 ΓΕΛ Αθηνών)" name="schoolName" data-val="true" data-val-required="This field is required *" id="schoolName" />
                                <label data-valmsg-for="schoolName" data-valmsg-replace="true" class="text-danger"></label>
                            </div>

                            <div class="col-12">
                                <input type="text" class="form-control" placeholder="Περιοχή" name="schoolArea" data-val="true" data-val-required="This field is required *" id="schoolArea" />
                                <label data-valmsg-for="schoolArea" data-valmsg-replace="true" class="text-danger"></label>
                                <hr>
                            </div>

                            <div class="col-12">
                                <label class="label-info">Στοιχεία εγγραφής στο σύστημα</label>
                            </div>

                            <div class="col-12">
                                <input type="email" class="form-control" placeholder="e-mail" name="Teacher.Email" data-val="true" data-val-required="This field is required *" id="Teacher_Email">
                                <label data-valmsg-for="Teacher.Email" data-valmsg-replace="true" class="text-danger"></label>
                            </div>

                            <div class="col-12">
                                <input type="password" class="form-control" placeholder="password" name="Teacher.Password" data-val="true" data-val-required="This field is required *" id="Teacher_Password">
                                <label data-valmsg-for="Teacher.Password" data-valmsg-replace="true" class="text-danger"></label>
                            </div>

                            <div class="col-12">
                                <input type="submit" class="btn btn-success" value="Σύνδεση" />
                                @if (Model.AccountExists != null && Model.AccountExists.Equals("True"))
                                {
                                    <label class="text-danger text-center">This account already exist!</label>
                                } else if (Model.SchoolExists != null && Model.SchoolExists.Equals("True"))
                                {
                                    <label class="text-danger text-center">This school already exist!</label>
                                }
                            </div>

                        </div>

                    </form>

                </div>

            </div>

        </div>
    </div>

    <h4><i class="fa fa-copyright h3"></i> Copyright Δημήτρης Μπεκιάρης</h4>
    <p>
        Το παρόν λογισμικό δεν αποτελεί σε καμία περίπτωση προιόν για χρήση. Αποτελεί μόνο ένα πρότυπο με το οποίο
        θα μπορούσε ένα πραγματικό λογισμικό να λειτουργεί.
    </p>

</body>

</html>